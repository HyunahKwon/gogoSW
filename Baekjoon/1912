#include <iostream>#include<algorithm>using namespace std;
int num_arr[100000];int sum_arr[100000];int max = 0;
void solve(int x) {  if (num_arr[x] < sum_arr[x - 1] + num_arr[x]) { //크면 max에 저걸 처넣겟지  sum_arr[x] = sum_arr[x - 1] + num_arr[x]; } else {  sum_arr[x] = sum_arr[x - 1];  if (sum_arr[x] < num_arr[x]) sum_arr[x] = num_arr[x]; }
}
int main() {
  int test_case; 
 cin >> test_case; int temp;  for (int i = 0; i < test_case; i++) {  cin >>  num_arr[i]; } // sum_arr[0] = num_arr[0]; int max = 0; max = num_arr[0]; for (int i = 1; i < test_case; i++) {  solve(i); }
 /*for (int i = 0; i < test_case; i++) {  cout << sum_arr[i] << " "; }*/ sort(sum_arr,sum_arr+test_case);  cout << sum_arr[test_case - 1];
 system("pause");
 return 0;}
